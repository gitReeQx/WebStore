@model IEnumerable<WebStore.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
    <h3>Сотрудники</h3>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Имя</th>
                <th>Фамилия</th>
                <th>Отчество</th>
                <th>Возраст</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <th>@employee.Id</th>
                    <th>@employee.FirstName</th>
                    <th>@employee.LastName</th>
                    <th>@employee.Patronymic</th>
                    <th>@employee.Age</th>
                </tr>
            }
        </tbody>
    </table>
    <br />
    <p><a href="~/Home">Главная</a></p>
</body>
</html>
